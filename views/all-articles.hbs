<main>
  <h1>Welcome to SoftUni Wiki!</h1>
  <section>
    <h2>All articles</h2>
    <div class="spacer">
      <ul>
        {{#each articles}}
        <li>
          <a href="/articles/{{this._id}}/details">{{this.title}}</a>
        </li>
        {{/each}}
      </ul>
    </div>
  </section>
</main>